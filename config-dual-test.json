{
  "database": {
    "type": "duckdb",
    "databases": [
      {
        "name": "duckdb",
        "type": "duckdb",
        "duckdb": {
          "path": "test_dual.duckdb",
          "memory_limit": "16GB",
          "threads": 8,
          "read_only": false,
          "bulk_mode": false,
          "disable_wal": false,
          "checkpoint_threshold": "1GB",
          "wal_auto_checkpoint": 1000,
          "temp_directory": "/tmp"
        }
      },
      {
        "name": "clickhouse",
        "type": "clickhouse",
        "clickhouse": {
          "host": "localhost",
          "port": 9000,
          "database": "nodelistdb",
          "username": "default",
          "password": "",
          "max_open_conns": 5,
          "max_idle_conns": 5,
          "conn_max_lifetime": "1h",
          "dial_timeout": "10s",
          "read_timeout": "30s",
          "write_timeout": "30s"
        }
      }
    ]
  }
}