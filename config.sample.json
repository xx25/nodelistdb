{
  "database": {
    "type": "duckdb",
    "duckdb": {
      "path": "./nodelist.duckdb",
      "memory_limit": "16GB",
      "threads": 8,
      "read_only": false
    },
    "clickhouse": {
      "host": "localhost",
      "port": 9000,
      "database": "nodelistdb",
      "username": "default",
      "password": "",
      "max_open_conns": 10,
      "max_idle_conns": 5,
      "conn_max_lifetime": "1h",
      "compression": "lz4",
      "dial_timeout": "30s",
      "read_timeout": "5m",
      "tls": false,
      "insecure": false,
      "cert_file": "",
      "key_file": "",
      "ca_file": ""
    }
  }
}